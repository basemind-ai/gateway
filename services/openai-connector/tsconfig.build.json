{
	"extends": "./tsconfig.json",
	"include": ["src/*.ts"],
	"exclude": ["node_modules", "dist"],
	"compilerOptions": {
		"allowJs": true,
		"baseUrl": ".",
		"declaration": false,
		"noEmit": false,
		"sourceMap": true,
		"module": "CommonJS",
		"moduleResolution": "node",
		"outDir": "dist",
		"skipLibCheck": true
	}
}
